<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.cuenca.resistencias.MainController">

    <ImageView fx:id="mainLogo" >
        <Image url="@img/uvlogo.jpg" ></Image>
    </ImageView>

    <Text text="Seleccione la resistencia que desea resolver:">
        <font><Font size="20" /></font>
    </Text>

    <Button text="Cubo de Resistencias" onAction="#showCuboResistencias" ></Button>
    <Button text="Octaedro de Resistencias" onAction="#showOctaedroResistencias"></Button>

</VBox>
